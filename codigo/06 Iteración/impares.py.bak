# -*- coding: utf-8 -*-

def par(n):
  """ Calcula el enésimo número par
  Parámetros:
    - n: número entero positivo
  """
  return 2*n


def impar(n):
  """ Calcula el enésimo número impar
  Parámetros:
    - n: número entero positivo
  """
  return 2*n+1

def imprime16pares():
  j = 0
  while j <= 15:
    print j , par(j)
    j = j +1

def imprime16impares():
  j = 0
  while j <= 15:
    print j , impar(j)
    j = j +1
  

print "Pares"
imprime16pares()

print "Impares"
imprime16impares()
